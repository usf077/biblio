<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="biblio-ejb" transaction-type="JTA">
		<jta-data-source>PostgresJNDIPool</jta-data-source>
		<class>fr.dauphine.lamsade.hib.biblio.modele.Auteur</class>
		<class>fr.dauphine.lamsade.hib.biblio.modele.Bibliographie</class>
		<class>fr.dauphine.lamsade.hib.biblio.modele.Commentaire</class>
		<class>fr.dauphine.lamsade.hib.biblio.modele.Confiance</class>
		<class>fr.dauphine.lamsade.hib.biblio.modele.Evaluation</class>
		<class>fr.dauphine.lamsade.hib.biblio.modele.TypeBibliographie</class>
		<class>fr.dauphine.lamsade.hib.biblio.modele.Utilisateur</class>
		 <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
    	<property name="javax.persistence.schema-generation.database.action" value="none"/></properties>
	</persistence-unit>
</persistence>
